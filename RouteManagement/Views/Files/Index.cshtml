@*@model IEnumerable<RouteManagement.Models.FileViewModel>*@

@model string[]

@{
	ViewData["Title"] = "Rotas";

	string[] services = new string[]
							{
		"INSTALAÇÃO",
		"MUDANÇA DE ENDEREÇO",
		"MUDANÇA DE LOCAL",
		"REFAZER MANUTENÇÃO",
		"REINSTALAÇÃO",
		"RETORNO CREDENCIADA",
		"SEM SINAL",
		"TROCA DE TERMINAL",
		"VISITA TECNICA"
				};
}

<h1>Rotas</h1>

<div class="row">
	<div class="col-md-4">
		<form asp-action="Create">
			<div asp-validation-summary="ModelOnly" class="text-danger"></div>

			<div class="form-group">
				<label class="control-label"> Times Disponíveis:</label><br />
				@foreach (var team in ViewBag.TeamsAvailable)
				{
					<input name="checkTeams" type="checkbox" value=@team.Id>

					<label for="@team.Name"> @team.Name</label>

					<br />
				}
			</div>

			<div class="form-group">
				<label class="control-label">Selecione os dados: </label><br />
				@foreach (var item in ViewBag.Headers)
				{
					<input name="checkData" type="checkbox" value=@item.ToString()>

					<label for="@item.ToString()"> @item.ToString()</label>

					<br />
				}
			</div>

			<div class="form-group">
				<input type="submit" value="Gerar Documento" class="btn btn-primary" />
			</div>
		</form>
	</div>
</div>