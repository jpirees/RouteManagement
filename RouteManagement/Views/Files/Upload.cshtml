@model RouteManagement.Models.FileViewModel

@{
    ViewData["Title"] = "Importar Arquivo";
}

@using (Html.BeginForm("Index", "Files", FormMethod.Post, new { enctype = "multipart/form-data" }))
{    
	<div class="text-center">
		<h4 class="display-4">Importar Rotas</h4>
		<br/>
			<div class="form-group">
				<input class="form-control" type="file" name="file" />
			</div>
			<div class="form-group">
				<input type="submit" value="Importar" class="btn btn-primary" />
			</div>
	</div>
}

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
